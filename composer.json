{
  "name": "etokar/plesk-ext-laravel",
  "description": "Integration package for Laravel queue management using the Plesk panel.",
  "keywords": [
    "laravel",
    "Plesk"
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/console": ">=7.0.0",
    "vlucas/phpdotenv": ">=4.0.0"
  },
  "require-dev": {
    "laravel/framework": "7.0.0"
  },
  "autoload": {
    "psr-4": {
      "PleskExtLaravel\\": "src/"
    }
  },
  "extra": {
    "component": "package",
    "laravel": {
      "providers": [
        "PleskExtLaravel\\Providers\\ConsoleServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
